{
  "name": "todolist",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --mode dev",
    "build": "cross-env NODE_ENV=production vue-cli-service build",
    "replaceEnv": "node ../web-service/utils/replaceEnv.js --originalBundlePath packages/todolist/output --targetBundlePath packages/todolist/dist --envPath packages/todolist",
    "copyDistFolder": "node ../web-service/utils/copy.js --originalBundlePath packages/todolist/dist --targetBundlePath packages/web-service/dist/todolist",
    "removeNodeModules": "shx rm -rf node_modules"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "cross-env": "^7.0.3",
    "uuidv4": "^6.2.13",
    "vue": "^3.2.13",
    "vue-i18n": "9",
    "vue-router": "4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "sass": "^1.69.7",
    "sass-loader": "^14.0.0"
  }
}
